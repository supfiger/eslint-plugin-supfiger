{
  "name": "eslint-plugin-supfiger",
  "version": "1.0.5",
  "description": "Custom and extended rules for ESLint by @supfiger",
  "keywords": [
    "ESLint",
    "eslintplugin",
    "eslint-plugin",
    "supfiger",
    "TypeScript",
    "no-explicit-any",
    "any",
    "catch"
  ],
  "author": "Danil Zinchenko (@supfiger)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supfiger/eslint-plugin-supfiger.git"
  },
  "bugs": {
    "url": "https://github.com/supfiger/eslint-plugin-supfiger/issues"
  },
  "homepage": "https://github.com/supfiger/eslint-plugin-supfiger#readme",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "mocha tests --recursive",
    "build": "rm -rf lib; tsc",
    "deploy": "yarn build && yarn publish",
    "reinstall": "rm -rf yarn.lock node_modules && yarn"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.16.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@types/requireindex": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.0.1",
    "eslint-plugin-eslint-plugin": "^4.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-supfiger": "^1.0.3",
    "mocha": "^9.1.3",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": "12.x || 14.x || >= 16"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "license": "ISC"
}
